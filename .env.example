# Example environment variables for Firebase client configuration
# Copy this to your .env file and replace with your actual values

# Firebase Web App Configuration (for client-side authentication)
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_APP_ID=your_firebase_app_id_here
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here

# Firebase Admin SDK Configuration (for server-side operations)
FIREBASE_PRIVATE_KEY=your_private_key_here
FIREBASE_CLIENT_EMAIL=your_service_account_email_here
FIREBASE_PRIVATE_KEY_ID=your_private_key_id_here

# Other environment variables
NODE_ENV=development
API_VERSION=/v1
API_PREFIX=/api
