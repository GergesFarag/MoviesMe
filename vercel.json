{
  "version": 2,
  "name": "ttom-project-api",
  "builds": [
    { "src": "api/app.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/app.ts" }
  ],
  "functions": {
    "api/app.ts": {
      "includeFiles": "**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
